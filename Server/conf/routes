# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /login/:params                  controllers.Application.login(params)
GET     /isInitialized/:params          controllers.Application.isInitialized(params)
GET     /initializeSystem/:params       controllers.Application.initializeSystem(params)
GET     /system_login/:params           controllers.Application.system_login(params)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
